   <center>
      <VBox fx:id="holderPane" styleClass="blanco" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
         </BorderPane.margin></VBox>
   </center>
   <left>
      <VBox styleClass="side" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#switchTickets" prefWidth="100.0" text="Tickets" />
            <Button mnemonicParsing="false" prefWidth="100.0" text="Button" />
            <Button mnemonicParsing="false" prefWidth="100.0" text="Button" />
            <Button mnemonicParsing="false" prefWidth="100.0" text="Button" />
            <Button mnemonicParsing="false" prefWidth="100.0" text="Button" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </left>
</BorderPane>
